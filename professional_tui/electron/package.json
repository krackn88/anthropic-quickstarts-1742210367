{
  "name": "cline-electron",
  "version": "1.0.0",
  "description": "Professional GUI client for Cline with integrated services",
  "main": "dist/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "compile": "tsc",
    "build": "tsc && electron-builder",
    "test": "jest"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.10.0",
    "@electron/remote": "^2.1.0",
    "@huggingface/inference": "^2.6.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.0",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.3.9",
    "keytar": "^7.9.0",
    "monaco-editor": "^0.45.0",
    "node-fetch": "^3.3.0",
    "octokit": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-monaco-editor": "^0.55.0",
    "react-router-dom": "^6.18.0",
    "stripe": "^14.3.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/electron": "^1.6.12",
    "@types/jest": "^29.5.14",
    "@types/keytar": "^4.4.2",
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "electron": "^35.0.2",
    "electron-builder": "^25.1.8",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0"
  },
  "build": {
    "appId": "com.cline.electron",
    "productName": "Cline",
    "directories": {
      "output": "dist"
    },
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
